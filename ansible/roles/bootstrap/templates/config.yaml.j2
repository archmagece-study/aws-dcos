bootstrap_url: http://{{ groups['dcos-bootstrap-' + zone][0] }}/bootstrap
cluster_name:  {{ dcos.cluster_name + ' - ' + zone|upper }}
exhibitor_storage_backend: static
master_discovery: static
master_list:
{% for item in groups['dcos-master-' + zone] %}
- {{ item }}
{% endfor %}
oauth_enabled: 'false'
process_timeout: 120
resolvers:
{% for item in dcos.resolvers %}
- {{ item }}
{% endfor %}
ssh_user: centos
